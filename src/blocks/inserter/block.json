{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpfn/inserter",
  "title": "FlashNotes Inserter",
  "category": "widgets",
  "icon": "index-card",
  "description": "Insert a FlashNotes card into a post.",
  "attributes": {
    "id": {
      "type": "string"
    },
    "block_id": {
      "type": "string"
    },
    "margin": {
      "type": "object",
      "default": {}
    },
    "padding": {
      "type": "object",
      "default": {}
    },
    "border": {
      "type": "object",
      "default": {}
    },
    "backgroundColor": {
      "type": "string",
      "default": ""
    },
    "hidden": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "html": false
  },
  "editorScript": "file:./index.js"
}
