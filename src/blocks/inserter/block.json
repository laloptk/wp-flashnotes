{
  "apiVersion": 3,
  "name": "wpfn/inserter",
  "title": "FlashNotes: Inserter",
  "category": "widgets",
  "icon": "sticky",
  "description": "Create or reference a study note and display it.",
  "supports": {
    "html": false,
    "anchor": true
  },
  "attributes": {
    "id": {
      "type": "number",
      "default": 0
    },
    "title": {
      "type": "text", 
      "default": ""
    },
    "answers_json": {
      "type": "array",
      "default": []
    },
    "explanation": {
      "type": "string",
      "default": ""
    }
  },
  "editorScript": "wpfn-blocks",
  "style": "file:./style.css"
}