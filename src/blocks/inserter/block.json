{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpfn/inserter",
  "title": "FlashNotes: Card Inserter",
  "category": "widgets",
  "icon": "migrate",
  "description": "Insert a FlashNotes card into a post.",
  "attributes": {
    "id": {
      "type": "string"
    },
    "block_id": {
      "type": "string"
    },
    "card_block_id": {
      "type": "string"
    },
    "object_type": {
      "type": "string",
      "default": "card"
    },
    "margin": {
      "type": "object",
      "default": {}
    },
    "padding": {
      "type": "object",
      "default": {
        "top": "15px",
        "right": "15px",
        "bottom": "15px",
        "left": "15px"
      }
    },
    "border": {
      "type": "object",
      "default": {
        "color": "#aaa",
        "style": "solid",
        "width": "1px"
      }
    },
    "backgroundColor": {
      "type": "string",
      "default": "#fff"
    },
    "borderRadius": {
      "type": "object",
      "default": {
        "top": "25px",
        "right": "25px",
        "bottom": "25px",
        "left": "25px"
      }
    },
    "hidden": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "html": false
  },
  "editorScript": "file:./index.js",
  "style": "file:../../style-index.css",
  "editorStyle": "file:../../index.css"
}
