{
  "apiVersion": 3,
  "name": "wpfn/card",
  "title": "FlashNotes: Card",
  "category": "widgets",
  "icon": "sticky",
  "description": "Create or reference a study card and display it.",
  "supports": {
    "html": false,
    "anchor": true
  },
  "attributes": {
    "block_id": {
      "type": "string"
    },
    "question": {
      "type": "string",
      "default": ""
    },
    "explanation": {
      "type": "string",
      "default": ""
    },
    "answers_json": {
      "type": "array",
      "default": []
    },
    "setIds": {
      "type": "array",
      "items": {
        "type": "integer"
      },
      "default": []
    }
  },
  "editorScript": "wpfn-blocks",
  "style": "file:./style.css"
}
